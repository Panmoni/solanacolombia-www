{
  "name": "solanacolombia-www",
  "type": "module",
  "version": "0.1.1",
  "scripts": {
    "dev": "npm run db:pull && astro dev",
    "db:pull": "wrangler d1 export solana_builders --remote --output ./prod_dump.sql && rm -rf .wrangler/state/v3/d1 && wrangler d1 execute solana_builders --local --file=./prod_dump.sql && rm ./prod_dump.sql",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "engines": {
    "node": ">=18.20.8"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@cloudflare/workers-types": "^4.20251229.0",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-phantom": "^0.9.28",
    "@solana/wallet-adapter-solflare": "^0.6.32",
    "@solana/web3.js": "^1.98.4",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.6",
    "resend": "^6.6.0",
    "tailwindcss": "^4.1.18",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "wrangler": "^4.54.0"
  }
}